gg.setVisible(false)
gg.alert('Click game guardian again to open script')


function Main()
menu = gg.choice({
'HACK A',
'HACK B',
'HACK C',
'HACK D',
'𝗘𝗫𝗜𝗧'},
nil,'💎Premium')
if menu == 1 then p1() end
if menu == 2 then p2() end
if menu == 3 then p3() end
if menu == 4 then p4() end
if menu == 5 then Exit() end
Premium=-1
end



function p1()
gg.alert('Hack A Activated')
end

function p2()
gg.alert('Hack B Activated')
end

function p3()
gg.alert('Hack C Activated')
end

function p4()
gg.alert('Hack D Activated')
end

function Exit()
print("")
os.exit()
end

cs = ''
while(true)do
if gg.isVisible(true) then
Premium=1
gg.setVisible(false)
end
gg.clearResults()
if Premium==1 then
Main()
end
end
