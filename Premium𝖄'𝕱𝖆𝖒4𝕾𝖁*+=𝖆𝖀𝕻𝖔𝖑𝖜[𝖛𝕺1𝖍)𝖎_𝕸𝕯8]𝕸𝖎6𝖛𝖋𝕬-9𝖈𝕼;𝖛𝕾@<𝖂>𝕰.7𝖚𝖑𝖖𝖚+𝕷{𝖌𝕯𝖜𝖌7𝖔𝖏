gg.alert("𝕄𝕒𝕕𝕖 𝔹𝕪 𝕌𝕟𝕜𝕟𝕠𝕨𝕟")

function Main()
menu = gg.choice({
'Test',
'Test',
'Test',
'Test',
'𝗘𝗫𝗜𝗧',
nil,'💎Premium')
if menu == 1 then p1() end
if menu == 2 then p2() end
if menu == 3 then p3() end
if menu == 4 then p4() end
if menu == 5 then Exit() end
GameGuardian=-1
end



function p1()
gg.alert('Hack A Activated')
end

function p2()
gg.alert('Hack B Activated')
end

function p3()
gg.alert('Hack C Activated')
end

function p4()
gg.alert('Hack D Activated')
end

function Exit()
print("𝕄𝕒𝕕𝕖 𝔹𝕪 𝕌𝕟𝕜𝕟𝕠𝕨𝕟")
os.exit()
end

cs = ''
while(true)do
if gg.isVisible(true) then
GameGuardian=1
gg.setVisible(false)
end
gg.clearResults()
if GameGuardian==1 then
Main()
end
end
